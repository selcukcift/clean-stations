{
  "name": "clean-stations",
  "version": "1.0.0",
  "description": "Torvan Medical CleanStation Configurator and Workflow App",
  "main": "src/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" --names \"Frontend,Backend\" --prefix-colors \"cyan,yellow\" --kill-others",
    "dev:frontend": "next dev -p 3005",
    "dev:backend": "node src/server.js",
    "dev:frontend-only": "next dev -p 3005",
    "dev:backend-only": "node src/server.js",
    "build": "next build",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\" --names \"Frontend,Backend\" --prefix-colors \"cyan,yellow\" --kill-others",
    "start:frontend": "next start -p 3005",
    "start:backend": "node src/server.js",
    "lint": "next lint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "node scripts/seed.js"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.9",
    "@types/react": "19.1.6",
    "bcryptjs": "^3.0.2",
    "concurrently": "^9.1.2",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.3.3",
    "prisma": "^6.8.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
