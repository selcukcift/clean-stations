    "framer-motion": "^12.15.0",
    "immer": "^10.1.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.511.0",
    "next": "^15.3.3",
    "next-auth": "^4.24.11",
    "postcss": "^8.5.4",
    "react": "^19.1.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "tailwind-merge": "^3.3.0",
